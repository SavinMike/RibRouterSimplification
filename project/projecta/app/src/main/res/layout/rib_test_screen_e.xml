<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rib_test_screen_e"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9C27B0"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test Screen E" />

    <Button
        android:id="@+id/screenE_global"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Open global ScreenA" />

    <Button
        android:id="@+id/screenE_local"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Open local ScreenA" />

    <FrameLayout
        android:id="@+id/testScreenE_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>
